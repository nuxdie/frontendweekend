    "devDependencies": {
        "davefeedread": "0.4.14",
        "markdownlint-cli": "0.13.0",
        "moment": "2.23.0",
        "node-fetch": "2.3.0",
        "prettier": "1.15.3",
        "remark-cli": "6.0.1",
        "remark-frontmatter": "1.3.1",
        "remark-lint": "6.0.4",
